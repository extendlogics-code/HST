{
  "name": "hst-backend",
  "version": "1.0.0",
  "description": "Backend for HST website",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "build:frontend": "cd ../frontend && npm install && npm run build",
    "setup:prod": "npm install && npm run build:frontend && npm run migrate && npm run seed"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "lodash": "^4.17.23",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.6.1",
    "puppeteer": "^24.36.0",
    "qrcode": "^1.5.4",
    "ua-parser-js": "^2.0.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
